---
import Layout from "../layouts/Layout.astro";
import Origin from "../components/Origin.astro";
import Carousel from "../components/Carousel.astro";
import HeroSection from "../components/HeroSection.astro";
import FaqSection from "../components/FaqSection.astro"
import TestimonialsSection from "../components/TestimonialsSection.astro";
import WaterFiltration from "../components/WaterFiltration.astro";
import global_settings from "../data/global_settings.json"
import home from "../data/home.json";
import testimonials from "../data/testimonials.json"
import faq from "../data/faq.json";
import CardHSW from "../components/CardHSW.astro"
import Sustainability from "../components/Sustainability.astro";
import Shop from "../components/Shop.astro";
import Contact from "../components/Contact.astro";
---

<Layout title={ global_settings.title }>

  <HeroSection
    content={ home.hero_content }
    settings={ global_settings }
    title={ home.hero_title }
  />

  <Origin />

  <Sustainability />

  <Shop />

  <Carousel
    title={ home.cases_title }
  />

  <FaqSection
    faq={ faq }
    title={ home.faq_title }
  />
  <Contact />
</Layout>

